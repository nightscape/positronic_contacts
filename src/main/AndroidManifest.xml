<manifest 
  android:versionName="0.1" 
  android:versionCode="1" 
  package="org.positronicnet.sample.contacts" 
  xmlns:android="http://schemas.android.com/apk/res/android">
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.WRITE_CONTACTS" />
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-sdk android:minSdkVersion="7"></uses-sdk>
    <application android:label="Positronic Contacts">
        <activity android:label="Positronic Contacts" android:name=".ContactsActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"></action>
                <category android:name="android.intent.category.LAUNCHER"></category>
            </intent-filter>
        </activity>
        <activity android:label="Edit Contact" 
                  android:name=".EditContactActivity"/>
    </application>
</manifest>
